{
  "name": "clonifier",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server -d --config webpack.dev.js",
    "prebuild": "mkdir dist; rm -rf dist/*; mkdir dist/tmp",
    "build": "webpack --config webpack.prod.js && webpack --config webpack.templates.js",
    "postbuild": "node dist/tmp/build_templates.js; rm -rf dist/tmp"
  },
  "devDependencies": {
    "@types/d3": "^5.7.2",
    "@types/file-saver": "^2.0.0",
    "@types/node": "^11.13.0",
    "@wasm-tool/wasm-pack-plugin": "0.2.3",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.2.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "d3": "^5.9.2",
    "dexie": "^2.0.4",
    "dialog-polyfill": "^0.4.10",
    "file-saver": "^2.0.1",
    "react": "16.9.0-alpha.0",
    "react-dom": "16.9.0-alpha.0",
    "react-window": "^1.7.1",
    "resize-observer-polyfill": "^1.5.1",
    "use-debounce": "0.0.9"
  }
